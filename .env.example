# Application
APP_NAME=python-ollama-hf-gguf-rag-chat
DEBUG=false

# Ollama Chat Model
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_CHAT_MODEL=hf.co/Qwen/Qwen3-8B-GGUF
OLLAMA_CHAT_TEMPERATURE=0.6
OLLAMA_CHAT_TOP_P=0.95
OLLAMA_CHAT_TOP_K=20
OLLAMA_CHAT_PRESENCE_PENALTY=1.5
# Enable thinking/reasoning mode for Qwen3 models (set to true to enable)
OLLAMA_CHAT_THINK=true

# Ollama Embedding Model
OLLAMA_EMBEDDING_MODEL=hf.co/Qwen/Qwen3-Embedding-8B-GGUF
OLLAMA_EMBEDDING_DIMENSIONS=4096

# PostgreSQL / PGVector
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# ETL
DOCUMENT_PATH=resources/2025-nfl-rulebook-final.pdf
CHUNK_SIZE=1000
CHUNK_OVERLAP=200

# RAG
RETRIEVAL_K=4
